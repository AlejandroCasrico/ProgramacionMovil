<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="136dp"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ToastActivity"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btn"
        android:layout_width="162dp"
        android:layout_height="69dp"
        android:layout_marginStart="64dp"
        android:text="hide"
        app:layout_constraintBottom_toTopOf="@+id/imc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imc" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="212dp"
        android:layout_height="91dp"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="24dp"
        android:text="show"
        app:layout_constraintBottom_toTopOf="@+id/segundoex"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.733" />

    <Button
        android:id="@+id/ToastActivity"
        android:layout_width="77dp"
        android:layout_height="54dp"
        android:layout_marginEnd="240dp"
        android:layout_marginBottom="72dp"
        android:text="toast"
        app:layout_constraintBottom_toTopOf="@+id/largo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toEndOf="@+id/button" />

    <Button
        android:id="@+id/largo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginBottom="56dp"
        android:text="Toast largo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/imc"
        android:layout_width="130dp"
        android:layout_height="95dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="68dp"
        android:text="imc"
        app:layout_constraintBottom_toTopOf="@+id/geo"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnjuego"
        android:layout_width="135dp"
        android:layout_height="59dp"
        android:layout_marginStart="148dp"
        android:layout_marginBottom="52dp"
        android:text="juego"
        app:layout_constraintBottom_toTopOf="@+id/btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/segundoex"
        app:layout_constraintTop_toBottomOf="@+id/btnanim"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="60dp"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/largo"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnanim"
        android:layout_width="124dp"
        android:layout_height="97dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="164dp"
        android:text="anim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.387"
        app:layout_constraintStart_toEndOf="@+id/btn2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/piedraypapel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginBottom="60dp"
        android:text="PPyt"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/geo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginBottom="60dp"
        android:text="geo"
        app:layout_constraintBottom_toTopOf="@+id/piedraypapel"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/Qrbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginTop="152dp"
        android:text="QR"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/geo"
        app:layout_constraintTop_toBottomOf="@+id/btn" />

    <Button
        android:id="@+id/dialogo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="276dp"
        android:layout_marginBottom="40dp"
        android:text="dialogo"
        app:layout_constraintBottom_toTopOf="@+id/ToastActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toEndOf="@+id/piedraypapel"
        app:layout_constraintTop_toBottomOf="@+id/Qrbtn"
        app:layout_constraintVertical_bias="0.38" />

    <Button
        android:id="@+id/segundoex"
        android:layout_width="193dp"
        android:layout_height="81dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="40dp"
        android:text="segundop"
        app:layout_constraintBottom_toTopOf="@+id/imc"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/pokemxd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="12dp"
        android:text="pokemon"
        app:layout_constraintBottom_toTopOf="@+id/imc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/segundoex"
        app:layout_constraintVertical_bias="0.906" />


</androidx.constraintlayout.widget.ConstraintLayout>
